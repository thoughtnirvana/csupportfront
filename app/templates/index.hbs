<div>
  <h1>Tickets</h1>
</div>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Owner</th>
      <th>Status</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
    {{#each model as |ticket|}}
      <tr class='ticket'>
        <td>{{ticket.name}}</td>
        <td>{{ticket.description}}</td>
        <td>{{ticket.owner}}</td>
        <td>{{ticket.current_status}}</td>
        <td></td>
      </tr>
    {{/each}}
  </tbody>
</table>

<a href="#" class="btn btn-info" role="button">Create Ticket</a>


